<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="242dp"
        tools:layout_editor_absoluteY="145dp">

        <TextView
            android:id="@+id/txt_first_name_profile_dialog"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:text="TextView" />

        <TextView
            android:id="@+id/txt_last_name_profile_dialog"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:text="TextView" />

        <TextView
            android:id="@+id/txt_country_profile_dialog"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:text="TextView" />

        <TextView
            android:id="@+id/txt_username_profile_dialog"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:text="TextView" />

        <Button
            android:id="@+id/btn_assign_profile_dialog"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="assign" />

        <Button
            android:id="@+id/btn_cancel_profile_dialog"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="cancel" />
    </LinearLayout>
</android.support.constraint.ConstraintLayout>